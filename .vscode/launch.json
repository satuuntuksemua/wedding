{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to Chrome",
            "port": 9222,
            "request": "attach",
            "type": "chrome",
            "webRoot": "${workspaceFolder}"
        },
      {
        "name": "Gatsby develop",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
        "args": ["develop"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      },
      {
        "name": "Gatsby build",
        "type": "node",
        "request": "launch",
        "protocol": "inspector",
        "program": "${workspaceRoot}/node_modules/gatsby/dist/bin/gatsby",
        "args": ["build"],
        "stopOnEntry": false,
        "runtimeArgs": ["--nolazy"],
        "sourceMaps": false
      }
    ]
  }